2020-02-18 11:04:09,370 [8] DEBUG PROGMGMT.Common.DataBase [(null)] - SQL:GetDataSet  String:  SELECT
SPODR.SUBNEGA_NO AS "サブネガNo",
SPODR.SPDPY_NO AS "呼出しNo",
CMNY.CMNY_NM AS "注文内容",
CUS.CUSTMER_RNM AS "発送先",
SPODR.BURANDO_NM AS "ブランド",
SPODR.SYOHIN_NM AS "品名",
SPODR.KAN_NM AS "容器型",
SPODR.ZUMEN_NO AS "図面No",
SPODR.LOTSU AS "ロット数",
SPODR.HANSU AS "版数",
( SELECT LISTAGG( CLR.COLOR_KJ, ' ') WITHIN GROUP (ORDER BY SPCLR.EDA_NO)
  FROM SPCLR_TBL SPCLR INNER JOIN COLOR_MST CLR ON CLR.COLOR_CD = SPCLR.COLOR_CD
  WHERE SPCLR.DPY_NO = SPODR.SPDPY_NO AND SPCLR.SPJSB_KBN = '1'
) AS "色名",
PROC.PROCESS_NM AS "工程",
CASE MGMT.PROCESS_CD || MGMT.SUBPROCESS_CD
    WHEN '0201' THEN '編集'
    WHEN '0202' THEN '検査'
    ELSE ''
END AS "サブ工程",
TO_CHAR(PLAN.YOTEI_DAY,'YYYY/MM/DD') AS "予定日",
TO_CHAR(MGMT.COMMIT_DATE,'YYYY/MM/DD') AS "作業日",
EMP.EMPLOYEE_NM AS "担当者名",
MGMT.WORKTIME_FROM AS "作業完了時間",
MGMT.OUT_HANSU AS "アウト版数",
MGMT.CASE_NO AS "ケースNo",
CASE MGMT.COLOR_FLG
    WHEN '1' THEN '済'
    ELSE ' '
END AS "色見本発送",
CASE MGMT.DOC_FLG
    WHEN '1' THEN '済'
    ELSE ' '
END AS "資料発送",
MGMT.WORK_MEMO AS "作業メモ",
MGMT.MEMO AS "メモ"
FROM SPODR_TBL SPODR
INNER JOIN PROGPLAN_SPN_TBL PLAN ON PLAN.DPY_NO = SPODR.SPDPY_NO
INNER JOIN PROGMGMT_SPN_TBL MGMT ON MGMT.DPY_NO = SPODR.SPDPY_NO AND MGMT.PROCESS_CD = PLAN.PROCESS_CD
LEFT OUTER JOIN CMNY_MST CMNY ON CMNY.CMNY_CD = SPODR.CMNY_CD
LEFT OUTER JOIN PROGPROCESS_MST PROC ON PROC.PROCESS_CD = MGMT.PROCESS_CD
LEFT OUTER JOIN PROGEMPLOYEE_MST EMP ON EMP.EMPLOYEE_CD = MGMT.EMPLOYEE_CD
LEFT OUTER JOIN CUSTMER_MST CUS ON CUS.CUSTMER_CD = SPODR.CUSTMER_CD
WHERE MGMT.COMMIT_DATE BETWEEN TO_DATE(:0,'RR-MM-DD') AND TO_DATE(:1,'RR-MM-DD')
ORDER BY SPODR.SPDPY_NO, SPODR.SUBNEGA_NO, MGMT.PROCESS_CD, MGMT.SUBPROCESS_CD, MGMT.COMMIT_DATE
